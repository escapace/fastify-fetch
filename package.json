{
  "name": "fastify-fetch",
  "description": "",
  "version": "0.0.0",
  "author": "escapace <opensource@escapace.com>",
  "bugs": "https://github.com/escapace/fastify-fetch/issues",
  "dependencies": {
    "fastify-plugin": "^3.0.1",
    "undici": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@ls-lint/ls-lint": "1.11.0",
    "@types/chai": "4.3.0",
    "@types/chai-as-promised": "7.1.5",
    "@types/fs-extra": "9.0.13",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "c8": "7.11.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "codecov": "3.8.2",
    "data-uri-to-buffer": "4.0.0",
    "esbuild": "0.14.31",
    "eslint": "8.12.0",
    "eslint-config-escapace": "3.12.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-editorconfig": "3.2.0",
    "eslint-plugin-no-null": "1.0.2",
    "fast-glob": "3.2.11",
    "fastify": "3.27.4",
    "fastify-multipart": "5.3.1",
    "fetch-blob": "3.1.5",
    "form-data": "4.0.0",
    "formdata-polyfill": "4.0.10",
    "fs-extra": "10.0.1",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "lint-staged": "12.3.7",
    "mocha": "9.2.2",
    "prettier": "2.6.2",
    "prettier-config-escapace": "1.0.4",
    "semantic-release": "19.0.2",
    "syncpack": "6.2.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">=14.19.1"
  },
  "exports": {
    ".": {
      "require": "./lib/node/index.cjs"
    }
  },
  "files": [
    "lib/types",
    "lib/node"
  ],
  "homepage": "https://github.com/escapace/fastify-fetch",
  "license": "MPL-2.0",
  "main": "./lib/node/index.cjs",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/escapace/fastify-fetch.git",
  "scripts": {
    "build": "rm -rf lib && mkdir -p lib/example lib/node lib/types && npm run build:node && npm run build:example && npm run build:types",
    "build:example": "esbuild src/example.ts --bundle --sourcemap --format=cjs --platform=node --external:fastify --external:fastify-plugin --external:undici --target=node14 --outfile=./lib/example/index.cjs",
    "build:node": "esbuild src/index.ts --bundle --sourcemap --format=cjs --external:fastify-plugin --external:undici --platform=node --target=node14 --outfile=./lib/node/index.cjs",
    "build:types": "tsc --emitDeclarationOnly --declarationDir lib/types && find ./lib/types \\( -name '*spec.d.ts' -o -name '*spec.d.ts.map' -o -name 'example.d.ts' -o -name 'example.d.ts.map' \\) -type f -delete",
    "lint": "ls-lint && eslint 'src/**/*.ts?(x)'",
    "prepare": "is-ci || husky install",
    "test": "node ./scripts/test.mjs && c8 mocha --enable-source-maps 'lib/tests/**/*.spec.cjs'",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "type": "commonjs",
  "types": "lib/types/index.d.ts"
}
