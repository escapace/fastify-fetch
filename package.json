{
  "name": "fastify-fetch",
  "description": "",
  "version": "0.0.0",
  "author": "escapace <opensource@escapace.com>",
  "bugs": "https://github.com/escapace/fastify-fetch/issues",
  "dependencies": {
    "@types/node-fetch": "^2.5.11",
    "data-uri-to-buffer": "^3.0.1",
    "fastify-plugin": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@ls-lint/ls-lint": "1.9.2",
    "@types/chai": "4.2.18",
    "@types/chai-as-promised": "7.1.4",
    "@types/mocha": "8.2.2",
    "@types/node": "15.6.1",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "c8": "7.7.3",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "codecov": "3.8.2",
    "esbuild": "0.12.5",
    "eslint": "7.27.0",
    "eslint-config-escapace": "3.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard-with-typescript": "20.0.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.1.0",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "fast-glob": "3.2.5",
    "fastify": "3.19.1",
    "fastify-multipart": "4.0.7",
    "fetch-blob": "3.1.2",
    "form-data": "3.0.1",
    "fs-extra": "9.1.0",
    "husky": "6.0.0",
    "is-ci": "3.0.0",
    "lint-staged": "11.0.0",
    "mocha": "8.4.0",
    "nyc": "15.1.0",
    "prettier": "2.3.0",
    "prettier-config-escapace": "1.0.0",
    "semantic-release": "17.4.3",
    "syncpack": "5.7.11",
    "ts-node": "10.1.0",
    "typescript": "4.3.2"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "files": [
    "lib/types",
    "lib/node"
  ],
  "homepage": "https://github.com/escapace/fastify-fetch",
  "license": "MPL-2.0",
  "main": "./lib/node/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/escapace/fastify-fetch.git",
  "scripts": {
    "build": "rm -rf lib && mkdir -p lib/example lib/node lib/types/temporary && npm run build:node && npm run build:example && npm run build:types",
    "build:example": "esbuild src/example.ts --bundle --sourcemap --format=cjs --platform=node --target=node14.17.0 --outfile=./lib/example/index.js",
    "build:node": "esbuild src/index.ts --bundle --sourcemap --format=cjs --external:data-uri-to-buffer --external:fastify-plugin --external:fetch-blob --platform=node --target=node14.17.0 --outfile=./lib/node/index.js",
    "build:types": "tsc --emitDeclarationOnly --declarationDir lib/types/temporary && mv ./lib/types/temporary/src/* ./lib/types && rm -rf ./lib/types/temporary && find ./lib/types \\( -name '*spec.d.ts' -o -name '*spec.d.ts.map' -o -name 'example.d.ts' -o -name 'example.d.ts.map' \\) -type f -delete",
    "lint": "ls-lint && eslint 'src/**/*.ts?(x)'",
    "prepare": "is-ci || husky install",
    "test": "node ./scripts/test.mjs && c8 mocha --enable-source-maps 'lib/tests/**/*.spec.js'",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "type": "commonjs",
  "types": "lib/types/index.d.ts"
}
