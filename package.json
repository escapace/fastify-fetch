{
  "name": "fastify-fetch",
  "description": "",
  "version": "0.0.0",
  "author": "escapace <opensource@escapace.com>",
  "bugs": "https://github.com/escapace/fastify-fetch/issues",
  "dependencies": {
    "fastify-plugin": "^4.2.1",
    "undici": "^5.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@ls-lint/ls-lint": "1.11.2",
    "@types/chai": "4.3.3",
    "@types/chai-as-promised": "7.1.5",
    "@types/fs-extra": "9.0.13",
    "@types/mocha": "9.1.1",
    "@types/node": "18.7.13",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "c8": "7.12.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "codecov": "3.8.2",
    "data-uri-to-buffer": "4.0.0",
    "esbuild": "0.15.5",
    "eslint": "8.22.0",
    "eslint-config-escapace": "3.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-editorconfig": "4.0.2",
    "eslint-plugin-no-null": "1.0.2",
    "fast-glob": "3.2.11",
    "fastify": "4.5.3",
    "fastify-multipart": "5.4.0",
    "fetch-blob": "3.2.0",
    "form-data": "4.0.0",
    "formdata-polyfill": "4.0.10",
    "fs-extra": "10.1.0",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "lint-staged": "13.0.3",
    "mocha": "10.0.0",
    "prettier": "2.7.1",
    "prettier-config-escapace": "1.0.4",
    "semantic-release": "19.0.5",
    "syncpack": "8.2.4",
    "ts-node": "10.9.1",
    "typescript": "4.8.2"
  },
  "engines": {
    "node": ">=16.14.2"
  },
  "exports": {
    ".": {
      "require": "./lib/cjs/index.cjs",
      "import": "./lib/esm/index.mjs"
    }
  },
  "files": [
    "lib/types",
    "lib/cjs",
    "lib/esm"
  ],
  "homepage": "https://github.com/escapace/fastify-fetch",
  "license": "MPL-2.0",
  "main": "lib/cjs/index.cjs",
  "module": "lib/esm/index.mjs",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/escapace/fastify-fetch.git",
  "scripts": {
    "build": "rm -rf lib && mkdir -p lib/example lib/node lib/types && npm run build:cjs && npm run build:esm && npm run build:example && npm run build:types",
    "build:cjs": "esbuild src/index.ts --bundle --sourcemap --format=cjs --external:fastify-plugin --external:undici --platform=node --target=node16 --outfile=./lib/cjs/index.cjs",
    "build:esm": "esbuild src/index.ts --bundle --sourcemap --format=esm --external:fastify-plugin --external:undici --platform=node --target=node16 --outfile=./lib/esm/index.mjs",
    "build:example": "esbuild src/example.ts --bundle --sourcemap --format=esm --platform=node --external:fastify --external:fastify-plugin --external:undici --target=node16 --outfile=./lib/example/index.mjs",
    "build:types": "tsc --emitDeclarationOnly --declarationDir lib/types && find ./lib/types \\( -name '*spec.d.ts' -o -name '*spec.d.ts.map' -o -name 'example.d.ts' -o -name 'example.d.ts.map' \\) -type f -delete",
    "lint": "ls-lint && eslint 'src/**/*.ts?(x)'",
    "prepare": "is-ci || husky install",
    "test": "node ./scripts/test.mjs && c8 mocha --enable-source-maps 'lib/tests/**/*.spec.mjs'",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "type": "commonjs",
  "types": "lib/types/index.d.ts"
}
