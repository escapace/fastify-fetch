{
  "name": "fastify-fetch",
  "description": "",
  "version": "0.0.0",
  "author": "escapace <opensource@escapace.com>",
  "bugs": "https://github.com/escapace/fastify-fetch/issues",
  "dependencies": {
    "fastify-plugin": "^4.5.0",
    "undici": "^5.22.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@fastify/multipart": "7.6.0",
    "@ls-lint/ls-lint": "2.0.0",
    "@types/chai": "4.3.5",
    "@types/chai-as-promised": "7.1.5",
    "@types/fs-extra": "11.0.1",
    "@types/mocha": "10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "arg": "5.0.2",
    "c8": "7.14.0",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "changelogithub": "0.12.11",
    "codecov": "3.8.2",
    "data-uri-to-buffer": "5.0.1",
    "esbuild": "0.17.19",
    "eslint": "8.41.0",
    "eslint-config-escapace": "3.17.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-editorconfig": "4.0.3",
    "eslint-plugin-no-null": "1.0.2",
    "execa": "7.1.1",
    "fast-glob": "3.2.12",
    "fastify": "4.17.0",
    "fetch-blob": "4.0.0",
    "form-data": "4.0.0",
    "formdata-polyfill": "4.0.10",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "lint-staged": "13.2.2",
    "mocha": "10.2.0",
    "prettier": "2.8.8",
    "prettier-config-escapace": "1.0.5",
    "semantic-release": "21.0.2",
    "semver": "7.5.1",
    "syncpack": "10.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">= 18.2.0",
    "pnpm": ">= 8.6.0"
  },
  "exports": {
    ".": {
      "require": "./lib/cjs/index.cjs",
      "import": "./lib/esm/index.mjs",
      "types": "./lib/types/index.d.ts"
    }
  },
  "files": [
    "lib/types",
    "lib/cjs",
    "lib/esm"
  ],
  "homepage": "https://github.com/escapace/fastify-fetch",
  "license": "MPL-2.0",
  "main": "lib/cjs/index.cjs",
  "module": "lib/esm/index.mjs",
  "private": false,
  "repository": "https://github.com/escapace/fastify-fetch.git",
  "scripts": {
    "build": "rm -rf lib && mkdir -p lib/example lib/node lib/types && npm run build:cjs && npm run build:esm && npm run build:example && npm run build:types",
    "build:cjs": "esbuild src/index.ts --bundle --sourcemap --format=cjs --external:fastify-plugin --external:undici --platform=node --target=node18 --outfile=./lib/cjs/index.cjs",
    "build:esm": "esbuild src/index.ts --bundle --sourcemap --format=esm --external:fastify-plugin --external:undici --platform=node --target=node18 --outfile=./lib/esm/index.mjs",
    "build:example": "esbuild src/example.ts --bundle --sourcemap --format=esm --platform=node --external:fastify --external:fastify-plugin --external:undici --target=node18 --outfile=./lib/example/index.mjs",
    "build:types": "tsc --emitDeclarationOnly --declarationDir lib/types && find ./lib/types \\( -name '*spec.d.ts' -o -name '*spec.d.ts.map' -o -name 'example.d.ts' -o -name 'example.d.ts.map' \\) -type f -delete",
    "lint": "ls-lint && eslint 'src/**/*.ts?(x)'",
    "prepare": "is-ci || husky install",
    "release": "node ./scripts/release.mjs",
    "test": "node ./scripts/test.mjs && c8 mocha --enable-source-maps 'lib/tests/**/*.spec.mjs'",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "type": "commonjs",
  "types": "lib/types/index.d.ts"
}
